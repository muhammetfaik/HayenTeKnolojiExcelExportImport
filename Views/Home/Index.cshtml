@{
    ViewData["Title"] = "Home Page";
}

<a asp-action="Download" asp-controller="Product">Export Excel</a>
<a asp-action="Sqlconnectioncontrol" asp-controller="Home">Sql Connection Control</a>
<a asp-action="createdata" asp-controller="Product">Create Data</a>

<div class="row">
    <div class="col-5">
        <form method="post" enctype="multipart/form-data" asp-controller="Product" asp-action="Upload">
            <div class="form-group">
                <div class="col-md-10">
                    <h3>Upload File</h3>
                    <input class="form-control" name="formFile" multiple="multiple" type="file" />
                </div> 
            </div>
            <br/>
            <div class="form-group">
                <div class="col-md-10">
                    <input class="btn btn-primary" type="submit" value="Upload File" />
                </div>
            </div>
        </form>
    </div>
</div>
